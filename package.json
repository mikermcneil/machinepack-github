{
  "name": "machinepack-github",
  "version": "4.5.0",
  "description": "Communicate with the Github API to get repos, commits, etc.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "machinepack",
    "machines",
    "github",
    "github-api"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "devDependencies": {
    "mocha": "~1.19.0",
    "test-machinepack-mocha": "^2.1.1"
  },
  "dependencies": {
    "github": "~0.1.16",
    "lodash": "^3.8.0",
    "machine": "^11.0.3",
    "machinepack-datetime": "^1.4.0",
    "machinepack-http": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mikermcneil/machinepack-github.git"
  },
  "machinepack": {
    "machineDir": "machines",
    "machines": [
      "get-repo",
      "create-repo",
      "get-user-details",
      "list-organization-repos",
      "list-repo-commits-at-path",
      "list-repo-commits",
      "list-repo-activity",
      "list-repos",
      "build-installable-url",
      "get-login-url",
      "get-access-token",
      "get-current-user",
      "parse-repo-str",
      "search-issues",
      "close-issue",
      "build-github-search-string",
      "comment-on-issue",
      "get-download-url",
      "add-labels-to-issue",
      "remove-label-from-issue",
      "list-issue-comments",
      "reopen-issue",
      "create-webhook",
      "edit-webhook",
      "list-webhooks",
      "add-collaborator"
    ],
    "devLinks": [
      "https://developer.github.com/v3/",
      "http://mikedeboer.github.io/node-github"
    ]
  }
}
