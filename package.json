{
  "name": "machinepack-github",
  "version": "5.0.0-1",
  "description": "Communicate with the Github API to get repos, commits, etc.",
  "scripts": {
    "test": "(npm run lint || echo 'Tolerating lint errors for now...') && npm run custom-tests && echo 'Done.'",
    "lint": "node ./node_modules/eslint/bin/eslint . --max-warnings=0 --report-unused-disable-directives && echo 'âœ”  Your .js files look good.'",
    "custom-tests": "node ./node_modules/mocha/bin/mocha && node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "machinepack",
    "machines",
    "github",
    "github-api"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "devDependencies": {
    "eslint": "4.11.0",
    "mocha": "^1.19.0",
    "test-machinepack-mocha": "^3.0.0"
  },
  "dependencies": {
    "@sailshq/lodash": "^3.10.2",
    "github": "0.1.16",
    "machine": "^15.2.2",
    "machinepack-datetime": "^1.4.0",
    "machinepack-http": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mikermcneil/machinepack-github.git"
  },
  "machinepack": {
    "machineDir": "lib",
    "machines": [
      "get-repo",
      "create-repo",
      "get-user-details",
      "list-organization-repos",
      "list-repo-commits-at-path",
      "list-repo-commits",
      "list-repo-activity",
      "list-repos",
      "build-installable-url",
      "get-login-url",
      "get-access-token",
      "get-current-user",
      "parse-repo-str",
      "search-issues",
      "close-issue",
      "build-github-search-string",
      "comment-on-issue",
      "get-download-url",
      "add-labels-to-issue",
      "remove-label-from-issue",
      "list-issue-comments",
      "reopen-issue",
      "create-webhook",
      "edit-webhook",
      "list-webhooks",
      "add-collaborator"
    ],
    "devLinks": [
      "https://developer.github.com/v3/",
      "http://mikedeboer.github.io/node-github"
    ]
  },
  "engines": {
    "node": ">=8"
  }
}
