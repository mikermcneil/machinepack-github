{
  "name": "machinepack-github",
  "version": "3.0.4",
  "description": "Communicate with the Github API to get repos, commits, etc.",
  "scripts": {
    "test": "node ./node_modules/test-machinepack-mocha/bin/testmachinepack-mocha.js"
  },
  "keywords": [
    "machinepack",
    "machines",
    "github",
    "github-api"
  ],
  "author": "Mike McNeil",
  "license": "MIT",
  "devDependencies": {
    "mocha": "~1.19.0",
    "test-machinepack-mocha": "^0.2.2"
  },
  "dependencies": {
    "github": "~0.1.16",
    "lodash": "^3.8.0",
    "machine": "^8.0.0",
    "machinepack-http": "^1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mikermcneil/machinepack-github.git"
  },
  "machinepack": {
    "machineDir": "machines",
    "machines": [
      "get-repo",
      "create-repo",
      "get-user-details",
      "list-organization-repos",
      "list-repo-commits-at-path",
      "list-repo-commits",
      "list-repo-activity",
      "list-repos",
      "build-installable-url",
      "get-login-url",
      "get-access-token",
      "get-current-user",
      "download-url",
      "parse-repo-str"
    ],
    "devLinks": [
      "https://developer.github.com/v3/",
      "http://mikedeboer.github.io/node-github"
    ]
  }
}
